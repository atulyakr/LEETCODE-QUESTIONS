Question link: "https://leetcode.com/problems/build-array-from-permutation/"

TIME: 93.97%   MEMORY: 17.13%

class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        vector<int>ans;
        for(int i=0;i<nums.size();i++){
            int x=nums[i];
            ans.push_back(nums[x]);
        }
        return ans;
    }
};
